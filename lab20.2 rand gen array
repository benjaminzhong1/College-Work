#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
  srand(time(0));
  int i=rand()%9+1;
  int ben[i][i];
  cout<<"2d array of size: "<<i<<" x "<<i<<endl;
  for(int x=0;x<i;x++)
    {
      for(int y=0;y<i;y++)
        {
          ben[x][y]=rand()%100;
          cout<<ben[x][y]<<"  ";
        }
      cout<<endl;
    }
  return 0;
}
